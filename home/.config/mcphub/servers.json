{
  "mcpServers": {
    "playwright": {
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ],
      "disabled": false,
      "command": "npx"
    },
    "browserbase": {
      "args": [
        "-y",
        "github.com/browserbase/mcp-server-browserbase"
      ],
      "disabled": true,
      "command": "npx"
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    },
    "puppeteer": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "disabled": true,
      "command": "npx"
    }
  }
}